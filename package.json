    "@types/jest": "^26.0.24",
    "@types/joi": "^17.2.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.3.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chalk": "^5.0.0",
    "eslint": "^8.5.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
