    "start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/infra/http/server.js",
    "prod": "NODE_ENV=production npx nodemon --ignore './data' --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/infra/http/server.ts\"",
    "dev": "NODE_ENV=development npx nodemon --ignore './data' --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/infra/http/server.ts\"",
    "lint": "eslint . --ext .ts -f ./eslint-formatter.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
